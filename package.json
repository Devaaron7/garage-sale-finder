{
  "name": "garage-sale-finder",
  "version": "0.1.0",
  "private": true,
  "homepage": "https://devaaron7.github.io/garage-sale-finder/",
  "dependencies": {
    "@emailjs/browser": "^4.4.1",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/react": "^14.2.1",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.126",
    "@types/react": "^18.3.23",
    "@types/react-dom": "^18.3.7",
    "@types/styled-components": "^5.1.34",
    "axios": "^1.9.0",
    "cheerio": "^1.0.0",
    "chromedriver": "^137.0.0",
    "cors": "^2.8.5",
    "craigslist": "^1.0.0",
    "express": "^5.1.0",
    "node-craigslist": "^2.3.1",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-icons": "^4.12.0",
    "react-router-dom": "^7.6.1",
    "react-scripts": "5.0.1",
    "selenium-webdriver": "^4.33.0",
    "styled-components": "^6.1.18",
    "typescript": "^4.9.5",
    "web-vitals": "^2.1.4"
  },
  "jest": {
    "testMatch": [
      "**/__tests__/**/*.test.[jt]s?(x)"
    ]
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --testMatch=**/__tests__/**/*.test.[jt]s?(x)",
    "test:integration": "react-scripts test --testMatch=**/__tests__/integration/**/*.test.[jt]s?(x)",
    "test:e2e": "cypress run",
    "test:all": "npm run test:integration && npm run test:e2e",
    "cypress:open": "cypress open",
    "cypress:open:e2e": "cypress open --e2e",
    "cypress:open:component": "cypress open --component",
    "eject": "react-scripts eject",
    "server": "node server/index.js",
    "dev": "concurrently \"npm run start\" \"npm run server\"",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build"
  },
  "proxy": "http://localhost:3001",
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@cypress/react": "^9.0.1",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "concurrently": "^9.1.2",
    "cypress": "^14.4.1",
    "gh-pages": "^6.3.0",
    "jest": "^27.5.1",
    "jest-environment-jsdom": "^30.0.0-beta.3",
    "msw": "^2.9.0"
  }
}
